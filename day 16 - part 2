h = open("C:/Projecten/d16.txt").read()*10000
i = (h[int(h[0:7]):])
for a in range(100):
    print(a)
    string = '' 
    e = 0
    while e < len(i):
        if e == 0:
            total = 0
            f = 0
            while f < len(i):
                total += int(i[f])
                f+=1
        elif e > 0:
            total -= int(i[e-1])
        string += str(total)[-1]
        e+=1
    i = string
print(i[0:8])           
