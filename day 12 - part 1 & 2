Iposx = -5
Iposy = 6
Iposz = -11
Ivelx = 0
Ively = 0
Ivelz = 0
Ipostellerx = 0
Ipostellery = 0
Ipostellerz = 0

Eposx = -8
Eposy = -4
Eposz = -2
Evelx = 0
Evely = 0
Evelz = 0
Epostellerx = 0
Epostellery = 0
Epostellerz = 0

Gposx = 1
Gposy = 16
Gposz = 4
Gvelx = 0
Gvely = 0
Gvelz = 0
Gpostellerx = 0
Gpostellery = 0
Gpostellerz = 0

Cposx = 11
Cposy = 11
Cposz = -4
Cvelx = 0
Cvely = 0
Cvelz = 0
Cpostellerx = 0
Cpostellery = 0
Cpostellerz = 0

Ipos = (Iposx, Iposy, Iposz)
Ivel = (Ivelx, Ively, Ivelz)

Epos = (Eposx, Eposy, Eposz)
Evel = (Evelx, Evely, Evelz)

Gpos = (Gposx, Gposy, Gposz)
Gvel = (Gvelx, Gvely, Gvelz)

Cpos = (Cposx, Cposy, Cposz)
Cvel = (Cvelx, Cvely, Cvelz)

I = (Ipos, Ivel)
E = (Epos, Evel)
G = (Gpos, Gvel)
C = (Cpos, Cvel)

manen = (I, E, G, C)

stappen = []

stappen.append(manen)
teller = 0

for e in range(1000):
    teller += 1
    if Iposx > Eposx:
        Ipostellerx -= 1
        Epostellerx += 1
    elif Iposx < Eposx:
        Ipostellerx += 1
        Epostellerx -= 1
    if Iposx > Gposx:
        Ipostellerx -= 1
        Gpostellerx += 1
    elif Iposx < Gposx:
        Ipostellerx += 1
        Gpostellerx -= 1
    if Iposx > Cposx:
        Ipostellerx -= 1
        Cpostellerx += 1
    elif Iposx < Cposx:
        Ipostellerx += 1
        Cpostellerx -= 1
    if Eposx > Gposx:
        Epostellerx -= 1
        Gpostellerx += 1
    elif Eposx < Gposx:
        Epostellerx += 1
        Gpostellerx -= 1
    if Eposx > Cposx:
        Epostellerx -= 1
        Cpostellerx += 1
    elif Eposx < Cposx:
        Epostellerx += 1
        Cpostellerx -= 1
    if Gposx > Cposx:
        Gpostellerx -= 1
        Cpostellerx += 1
    elif Gposx < Cposx:
        Gpostellerx += 1
        Cpostellerx -= 1


    if Iposy > Eposy:
        Ipostellery -= 1
        Epostellery += 1
    elif Iposy < Eposy:
        Ipostellery += 1
        Epostellery -= 1
    if Iposy > Gposy:
        Ipostellery -= 1
        Gpostellery += 1
    elif Iposy < Gposy:
        Ipostellery += 1
        Gpostellery -= 1
    if Iposy > Cposy:
        Ipostellery -= 1
        Cpostellery += 1
    elif Iposy < Cposy:
        Ipostellery += 1
        Cpostellery -= 1
    if Eposy > Gposy:
        Epostellery -= 1
        Gpostellery += 1
    elif Eposy < Gposy:
        Epostellery += 1
        Gpostellery -= 1
    if Eposy > Cposy:
        Epostellery -= 1
        Cpostellery += 1
    elif Eposy < Cposy:
        Epostellery += 1
        Cpostellery -= 1
    if Gposy > Cposy:
        Gpostellery -= 1
        Cpostellery += 1
    elif Gposy < Cposy:
        Gpostellery += 1
        Cpostellery -= 1


    if Iposz > Eposz:
        Ipostellerz -= 1
        Epostellerz += 1
    elif Iposz < Eposz:
        Ipostellerz += 1
        Epostellerz -= 1
    if Iposz > Gposz:
        Ipostellerz -= 1
        Gpostellerz += 1
    elif Iposz < Gposz:
        Ipostellerz += 1
        Gpostellerz -= 1
    if Iposz > Cposz:
        Ipostellerz -= 1
        Cpostellerz += 1
    elif Iposz < Cposz:
        Ipostellerz += 1
        Cpostellerz -= 1
    if Eposz > Gposz:
        Epostellerz -= 1
        Gpostellerz += 1
    elif Eposz < Gposz:
        Epostellerz += 1
        Gpostellerz -= 1
    if Eposz > Cposz:
        Epostellerz -= 1
        Cpostellerz += 1
    elif Eposz < Cposz:
        Epostellerz += 1
        Cpostellerz -= 1
    if Gposz > Cposz:
        Gpostellerz -= 1
        Cpostellerz += 1
    elif Gposz < Cposz:
        Gpostellerz += 1
        Cpostellerz -= 1
        
    Iposx = Iposx + Ipostellerx + Ivelx
    Iposy = Iposy + Ipostellery + Ively
    Iposz = Iposz + Ipostellerz + Ivelz

    Eposx = Eposx + Epostellerx + Evelx
    Eposy = Eposy + Epostellery + Evely
    Eposz = Eposz + Epostellerz + Evelz

    Gposx = Gposx + Gpostellerx + Gvelx
    Gposy = Gposy + Gpostellery + Gvely
    Gposz = Gposz + Gpostellerz + Gvelz

    Cposx = Cposx + Cpostellerx + Cvelx
    Cposy = Cposy + Cpostellery + Cvely
    Cposz = Cposz + Cpostellerz + Cvelz

    Ivelx = Ivelx + Ipostellerx
    Ively = Ively + Ipostellery
    Ivelz = Ivelz + Ipostellerz

    Evelx = Evelx + Epostellerx
    Evely = Evely + Epostellery
    Evelz = Evelz + Epostellerz

    Gvelx = Gvelx + Gpostellerx
    Gvely = Gvely + Gpostellery
    Gvelz = Gvelz + Gpostellerz

    Cvelx = Cvelx + Cpostellerx
    Cvely = Cvely + Cpostellery
    Cvelz = Cvelz + Cpostellerz

    Ipos = (Iposx, Iposy, Iposz)
    Ivel = (Ivelx, Ively, Ivelz)

    Epos = (Eposx, Eposy, Eposz)
    Evel = (Evelx, Evely, Evelz)

    Gpos = (Gposx, Gposy, Gposz)
    Gvel = (Gvelx, Gvely, Gvelz)

    Cpos = (Cposx, Cposy, Cposz)
    Cvel = (Cvelx, Cvely, Cvelz)

    I = (Ipos, Ivel)
    E = (Epos, Evel)
    G = (Gpos, Gvel)
    C = (Cpos, Cvel)

    manen = (I, E, G, C)
    
    stappen.append(manen)

    Ipostellerx = 0
    Ipostellery = 0
    Ipostellerz = 0

    Epostellerx = 0
    Epostellery = 0
    Epostellerz = 0

    Gpostellerx = 0
    Gpostellery = 0
    Gpostellerz = 0

    Cpostellerx = 0
    Cpostellery = 0
    Cpostellerz = 0


telop = 0
telop2 = 0
enpot = []
enkin = []
energy = 0
for e in stappen[1000]:
    for f in e[0]:
        telop += abs(f)
    enpot.append(telop)
    telop = 0
    for f in e[1]:
        telop += abs(f)
    enkin.append(telop)
    telop = 0
    
for e in range(0, 4):
    energy += (enpot[e] * enkin[e])


#part 1:
print(energy)
#13399

what = []
for e in range(len(stappen)):
    if e > 0:
        if stappen[e][0][0][0] == stappen[0][0][0][0] and stappen[e][1][0][0] == stappen[0][1][0][0] and stappen[e][2][0][0] == stappen[0][2][0][0] and stappen[e][3][0][0] == stappen[0][3][0][0]:
            if e%2 == 0 and "x" not in what:
                what.append("x")
                xcycle = e
        elif stappen[e][0][0][1] == stappen[0][0][0][1] and stappen[e][1][0][1] == stappen[0][1][0][1] and stappen[e][2][0][1] == stappen[0][2][0][1] and stappen[e][3][0][1] == stappen[0][3][0][1]:
            if e%2 == 0 and "y" not in what:
                what.append("y")
                ycycle = e
        elif stappen[e][0][0][2] == stappen[0][0][0][2] and stappen[e][1][0][2] == stappen[0][1][0][2] and stappen[e][2][0][2] == stappen[0][2][0][2] and stappen[e][3][0][2] == stappen[0][3][0][2]:
            if e%2 == 0 and "z" not in what:
                what.append("z")
                zcycle = e

combo_xy = xcycle
while combo_xy%ycycle != 0:
    combo_xy += xcycle

combo_xz = xcycle
while combo_xz%zcycle != 0:
    combo_xz += xcycle
    
combo_xyz = combo_xy
while combo_xyz%combo_xz != 0:
    combo_xyz += combo_xy

#part 2:
print(combo_xyz)
