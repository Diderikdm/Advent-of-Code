    covely3.append(covely2)
    covelz3.append(covelz2)
    x += 4

from collections import defaultdict
D = defaultdict(list)
for i,item in enumerate(coposy3):
    
    D[item].append(i)
D = {k:v for k,v in D.items() if len(v)>1}

print(D)

