       coords.append(f[0])
       covels.append(f[1])





x = 0
coords3 = []
covels3 = []
for e in range(len(stappen)):
    coords2 = (coords[x], coords[x + 1], coords[x + 2], coords[x + 3])
    covels2 = (covels[x], covels[x + 1], covels[x + 2], covels[x + 3])
    coords3.append([coords2])
    covels3.append([covels2])
    x += 4


x = 0
coposx = []
